var assert = require('assert');

var calc = require('./src/assets/calc/calc.js');

describe('Calculator Tests', function() {

	it('returns 1+1=2', function(done) {
		assert.equal(calc.calculate("1+1"), 2);
		done();
		});
	it('returns 2*2=4', function(done) {
		assert.equal("2*2", 4);
		done();
		});
});
